<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <remote name="roc-github"
fetch="https://github.com.cnpmjs.org/RadeonOpenCompute/" />
    <remote name="rocm-devtools"
fetch="https://github.com.cnpmjs.org/ROCm-Developer-Tools/" />
    <remote name="rocm-swplat"
fetch="https://github.com.cnpmjs.org/ROCmSoftwarePlatform/" />
    <remote name="gpuopen-libs"
fetch="https://github.com.cnpmjs.org/GPUOpen-ProfessionalCompute-Libraries/" />
    <remote name="gpuopen-tools"
fetch="https://github.com.cnpmjs.org/GPUOpen-Tools/" />
    <remote name="KhronosGroup"
fetch="https://github.com.cnpmjs.org/KhronosGroup/" />
    <default revision="refs/tags/rocm-4.3.0"
     remote="roc-github"
     sync-c="true"
     sync-j="4" />
<!--list of projects for ROCM-->
    <project name="ROCT-Thunk-Interface" />
    <project name="ROCR-Runtime" />
    <project name="rocm_smi_lib" />
    <project name="rocm-cmake" />
    <project name="rocminfo" /> 
    <project name="rocprofiler" remote="rocm-devtools" />
    <project name="roctracer" remote="rocm-devtools" />
    <project name="ROCm-OpenCL-Runtime" />
<!--HIP Projects-->
    <project name="HIP" remote="rocm-devtools" />
    <project name="HIP-Examples" remote="rocm-devtools" />
    <project name="ROCclr" remote="rocm-devtools" />
<!-- The following projects are all associated with the AMDGPU LLVM compiler -->
    <project name="ROCm-Device-Libs" />
    <project name="ROCm-CompilerSupport" />
    <project name="rocm_bandwidth_test" />
<!-- ROCm Libraries -->
    <project name="rocBLAS" remote="rocm-swplat" />      
    <project name="rocRAND" remote="rocm-swplat" />   
    <project name="ROCmValidationSuite" remote="rocm-devtools" />
</manifest>